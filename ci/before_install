cd ..
git clone --depth=50 --branch=freeswitch-stash git://github.com/davehorton/drachtio-server.git && cd drachtio-server
sed -i 's/git@github.com:/https:\/\/github.com\//' .gitmodules
git submodule update --init --recursive
./bootstrap.sh
mkdir build && cd $_
../configure
make
sudo make install
cd ../../drachtio-client